<aspectj>
    <aspects>
        <concrete-aspect name="ro.rodin.businessprocessesdiagram.aspect.MyLogMethodExecution" extends="ro.rodin.businessprocessesdiagram.aspect.MethodExecutionLogger">
            <pointcut name="executeMethodFromPackage" expression="execution(* ro.rodin.businessprocessesdiagram.logic..*(..))"/>
        </concrete-aspect>
        <concrete-aspect name="ro.rodin.businessprocessesdiagram.aspect.MyMethodExecutionBuilder" extends="ro.rodin.businessprocessesdiagram.aspect.MethodExecutionBuilder">
            <pointcut name="callMethodFromPackage" expression="call(* ro.rodin.businessprocessesdiagram.logic..*(..))"/>
        </concrete-aspect>
    </aspects>
    <!--<weaver options="-showWeaveInfo -Xlint:ignore">
    </weaver>-->
    <weaver options="-Xlint:ignore">
    </weaver>
</aspectj>